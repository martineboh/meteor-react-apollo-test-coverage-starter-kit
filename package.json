{
  "name": "meteor-starter-kit",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "lint": "eslint --quiet --ext .js,.jsx .",
    "prettier": "prettier --write \"client/**/*.js\" \"imports/**/*.js\" \"server/**/*.js\" --single-quote=true --trailing-comma=es5 --print-width=100",
    "lint": "npm run lint",
    "test": "meteor npm run test:app-unit && meteor npm run test:app-full",
    "coverage": "meteor npm run coverage:app-unit && meteor npm run coverage:app-full",
    "coverage:app-unit": "spacejam test --coverage 'out_lcovonly out_coverage out_html' --driver-package practicalmeteor:mocha-console-runner",
    "coverage:app-full": "spacejam test --full-app --coverage 'out_lcovonly out_coverage out_html in_coverage' --driver-package practicalmeteor:mocha-console-runner",
    "pretest": "npm run lint",
    "test:app-unit": "spacejam --driver-package=practicalmeteor:mocha-console-runner test",
    "test:app-unit-watch": "meteor --driver-package=practicalmeteor:mocha-console-runner test --port $IP:$PORT",
    "test:app-full": "spacejam --driver-package=practicalmeteor:mocha-console-runner test --full-app",
    "test:app-full-watch": "meteor --driver-package=practicalmeteor:mocha-console-runner test --full-app"
  },
  "dependencies": {
    "apollo-client": "^1.0.0-rc.3",
    "babel-runtime": "^6.18.0",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "graphql": "^0.9.0",
    "graphql-server-express": "^0.6.0",
    "graphql-subscriptions": "^0.3.1",
    "graphql-tag": "^1.3.1",
    "graphql-tools": "^0.11.0",
    "invariant": "^2.2.1",
    "meteor-node-stubs": "~0.2.0",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-apollo": "^1.0.0-rc.3",
    "react-dom": "^15.4.2",
    "redux": "^3.6.0",
    "subscriptions-transport-ws": "^0.5.5-alpha.0",
    "tracker-component": "^1.3.21",
    "spacejam": "https://github.com/serut/spacejam/tarball/windows-suppport-rc4"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "enzyme": "=2.4.1",
    "codecov.io": "^0.1.6",
    "eslint": "^3.17.1",
    "eslint-import-resolver-meteor": "^0.3.3",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-meteor": "4.0.0",
    "eslint-plugin-no-unused-vars-rest": "^1.0.4",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.0",
    "prettier": "^0.20.0"
  }
}
